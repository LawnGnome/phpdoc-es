  <refentry id='function.gmp-com'>
   <refnamediv>
    <refname>gmp_com</refname>
    <refpurpose>Calcula el complementario</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descripci&oacute;n</title>
     <methodsynopsis>
      <type>resource</type><methodname>gmp_com</methodname>
      <methodparam><type>resource</type><parameter>a</parameter></methodparam>
     </methodsynopsis>
    <para>
     Devuelve el complementario a 1 de <parameter>a</parameter>.
    </para>
    <example>
     <title>Ejemplo de <function>gmp_com</function></title>
     <programlisting role="php">
<![CDATA[
<?php
$com = gmp_com("1234");
echo gmp_strval($com) . "\n";
?>
]]>
     </programlisting>
     <para>
      La salida del anterior programa es:
     </para>
     <screen>
<![CDATA[
-1235
]]>
     </screen>
    </example>

   </refsect1>
  </refentry>

