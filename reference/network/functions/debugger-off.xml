<refentry xml:id="function.debugger-off" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>debugger_off</refname>
  <refpurpose>Deshabilita el depurador interno de PHP (PHP 3)</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>debugger_off</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Deshabilita el depurador interno de PHP.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
    Devuelve 1 en caso de éxito, 0 en caso de error.
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  <para>
   Esta función está disponible únicamente en PHP 3.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><link linkend="debugger">Depuración de PHP</link></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

