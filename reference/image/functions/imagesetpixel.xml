<!-- splitted from ./es/functions/image.xml, last change in rev 1.1 -->
<!-- last change to 'imagesetpixel' in en/ tree in rev 1.2 -->
  <refentry id="function.imagesetpixel">
   <refnamediv>
    <refname>ImageSetPixel</refname>
    <refpurpose>Dibuja un pixel</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descripci&oacute;n</title>
     <methodsynopsis>
     <type>int</type><methodname>imagesetpixel</methodname>
     <methodparam><type>int</type><parameter>im</parameter></methodparam>
     <methodparam><type>int</type><parameter>x</parameter></methodparam>
     <methodparam><type>int</type><parameter>y</parameter></methodparam>
     <methodparam><type>int</type><parameter>col</parameter></methodparam>
     </methodsynopsis>
    <para> 
     ImageSetPixel dibuja un pixel x,y (arriba izquierda 0,0) 
     en la imagen im con color col.</para> <para> Vea tambi&eacute;n
     <function>imagecreate</function> y
     <function>imagecolorallocate</function>.</para>
   </refsect1>
  </refentry>

