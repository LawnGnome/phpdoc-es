   <refnamediv>
    <refname>ldap_delete</refname>
    <refpurpose>Borra una entrada de un directorio</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descripci&oacute;n</title>
     <methodsynopsis>
      <type>bool</type><methodname>ldap_delete</methodname>
