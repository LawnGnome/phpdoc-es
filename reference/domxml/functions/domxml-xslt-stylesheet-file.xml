  <refentry id="function.domxml-xslt-stylesheet-file">
   <refnamediv>
    <refname>domxml_xslt_stylesheet_file</refname>
    <refpurpose>
     Crea un Objeto DomXsltStylesheet a partir de un documento XSL en
     un archivo
    </refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
     <methodsynopsis>
      <type>XsltStylesheet</type><methodname>domxml_xslt_stylesheet_file</methodname>
      <methodparam><type>string</type><parameter>archivo_xsl</parameter></methodparam>
     </methodsynopsis>
    &warn.undocumented.func;

    <para>
    </para>
    <para>
     Vea tambi&eacute;n
     <function>domxsltstylesheet->process</function>,
     <function>domxml_xslt_stylesheet</function>, y
     <function>domxml_xslt_stylesheet_doc</function>
    </para>
   </refsect1>
  </refentry>

