<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-getimageprofile">
 <refnamediv>
  <refname>Imagick::getImageProfile</refname>
  <refpurpose>Devuelve el perfil nominado de la imagen</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>Imagick::getImageProfile</methodname>
   <methodparam><type>string</type><parameter>name</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Devuelve el perfil nominado de la imagen.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>name</parameter></term>
     <listitem>
      <para>
      El nombre del perfil que se va a devolver.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve una cadena que contiene el perfil de la imagen.
   &imagick.imagickexception.throw;
  </para>
 </refsect1>

</refentry>

