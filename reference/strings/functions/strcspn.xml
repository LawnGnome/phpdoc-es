<!-- splitted from ./es/functions/strings.xml, last change in rev 1.7 -->
<!-- last change to 'strcspn' in en/ tree in rev 1.2 -->
  <refentry id="function.strcspn">
   <refnamediv>
    <refname>strcspn</refname>
    <refpurpose>
     Encuentra la longitud del elemento inicial que no coincide
     con la m&aacute;scara
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descripci&oacute;n</title>
     <methodsynopsis>
      <type>int</type><methodname>strcspn</methodname>
      <methodparam><type>string</type><parameter>cad1</parameter></methodparam>
      <methodparam><type>string</type><parameter>cad2</parameter></methodparam>
     </methodsynopsis>
    <simpara>
     Devuelve la longitud del segmento inicial de 
     <parameter>cad1</parameter> que <emphasis>no</emphasis>
     contiene ninguno de los caracteres de <parameter>cad2</parameter>.
    </simpara>
    <simpara>
     Vea tambi&eacute;n <function>strspn</function>.
    </simpara>
   </refsect1>
  </refentry>

