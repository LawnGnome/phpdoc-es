  <refentry xml:id="function.strrev" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>strrev</refname>
    <refpurpose>Invierte una cadena</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descripci&oacute;n</title>
     <methodsynopsis>
      <type>string</type><methodname>strrev</methodname>
      <methodparam><type>string</type><parameter>cadena</parameter></methodparam>
     </methodsynopsis>
    <para>
     Devuele la <parameter>cadena</parameter> invertida.
	<example>
      <title>Ejemplo de <function>strrev</function></title>
      <programlisting role="php">
<![CDATA[
<?php
echo strrev("Hola Mundo"); // La salida es: "odnuM aloH"
?>
]]>
      </programlisting>
     </example>
    </para>
   </refsect1>
  </refentry>

